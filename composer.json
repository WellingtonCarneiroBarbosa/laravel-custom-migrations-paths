{
    "name": "wellingtoncarneirobarbosa/laravel-custom-migrations-paths",
    "description": "Create custom paths for your laravel migrations",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WellingtonCarneiroBarbosa\\LaravelCustomMigrationsPaths\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Wellington Barbosa",
            "email": "owellingtoncarneirobarbosa@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "^9.17"
    },
    "extra": {
        "laravel": {
            "providers": [
                "WellingtonCarneiroBarbosa\\LaravelCustomMigrationsPaths\\Providers\\MigrationServiceProvider"
            ]
        }
    }
}
